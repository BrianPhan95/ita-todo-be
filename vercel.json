{
  "version": 2,
  "builds": [
      {
          "src": "build/main.js",
          "use": "@vercel/node"
      },
      {
        "src": "**/*.njk",
        "use": "@vercel/static"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "build/main.js",
          "methods": [ "GET", "POST", "PUT", "PATCH", "DELETE" ]
      }
  ]
}